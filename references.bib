	pages = {1049--1066},
	file = {Full Text:C\:\\Users\\5879167\\Zotero\\storage\\FNJHV8BV\\Goetgeluk et al. - 2008 - Estimation of Controlled Direct Effects.pdf:application/pdf},
}

