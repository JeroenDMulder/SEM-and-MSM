@techreport{green_adolescent_2022,
	type = {preprint},
	title = {The {Adolescent} {Wellbeing} {Paradigm} ({AWP}): {Psychometric} {Properties} and {Validation}},
	shorttitle = {The {Adolescent} {Wellbeing} {Paradigm} ({AWP})},
	url = {https://osf.io/vwn3s},
	abstract = {Few studies have attempted to capture the perspectives of the current generation of youth for understanding adolescent wellbeing. In the present multi-sample mixed method study we developed a novel assessment tool of adolescent wellbeing, the Adolescent Wellbeing Paradigm (AWP). In Study 1, an online survey study among Dutch-speaking adolescents and young adults (N = 339, Mage = 18.44 years, SD = 3.53, 79\% females) was conducted to inspect which initial AWP-items were viewed as being important for their wellbeing. In Study 2, we co-evaluated the original AWP-items and co-created new items with adolescents and young adults (N = 25) via focus groups. In Study 3, we examined the validity and reliability of the updated AWP in a new sample of youth (Mage = 19.86 years, SD = 4.46, 71\% females). Principal Components Analyses revealed five factors of adolescent wellbeing: 1) having impact, purpose, and meaning; 2) absence of (excessive) stress; 3) family relationships; 4) self-confidence; and 5) feeling respected, appreciated, and loved. These AWP-factors were related to other measures of mental health and wellbeing, and the AWP showed good internal consistency and test-retest reliability. In conclusion, the AWP was found to be a valid and reliable measure of adolescent wellbeing.},
	urldate = {2023-07-04},
	institution = {PsyArXiv},
	author = {Green, Kayla and Van De Groep, Suzanne and Van Der Cruijsen, Renske and Crone, Eveline},
	month = jul,
	year = {2022},
	doi = {10.31234/osf.io/vwn3s},
}

